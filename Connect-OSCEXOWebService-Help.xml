    <command:examples>
      <command:example>
        <maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
        <maml:introduction>
          <maml:para>PS C:\&gt;</maml:para>
        </maml:introduction>
        <dev:code>Connect-OSCEXOWebService -Credential (Get-Credential admin@domain01.onmicrosoft.com)</dev:code>
        <dev:remarks>
          <maml:para>Description</maml:para>
          <maml:para>-----------</maml:para>
