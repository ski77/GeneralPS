    <command:details>
      <command:name>Get-OSCEXODistributionGroupMember</command:name>
      <maml:description>
        <maml:para>Returns group members in distribution groups.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para></maml:para>
      </maml:copyright>
      <command:verb>Get</command:verb>
      <command:noun>OSCEXODistributionGroupMember</command:noun>
      <dev:version></dev:version>
    </command:details>
    <maml:description>
      <maml:para>The Get-OSCEXODistributionGroupMember will attempt to return group members in distribution groups. Additionally you can return members in nested distribution groups.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Get-OSCEXODistributionGroupMember</maml:name>
        <command:parameter globbing="false" variableLength="false" position="1" required="true" pipelineInput="true">
          <maml:name>Identity</maml:name>
          <maml:description>
            <maml:para>Specifies the email address of a distribution group.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
        <command:parameter globbing="false" variableLength="false" position="named" required="false" pipelineInput="false">
          <maml:name>Recurse</maml:name>
          <maml:description>
            <maml:para>Gets members from distribution group and nested distribution groups.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">switch</command:parameterValue>
        </command:parameter>
        <command:parameter globbing="false" variableLength="false" position="named" required="false" pipelineInput="false">
          <maml:name>AllProperty</maml:name>
          <maml:description>
            <maml:para>Returns all member properties.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">switch</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
